{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ElasticSearch": {
    "Host": "http://elasticsearch1:9200"
  },
  "Kafka": {
    "servers": "kafka1:19092,kafka2:19093,kafka3:19094"
  },
  "Consumers": {
    "Rating": {
      "MessagesPerCycle": 300,
      "MaxTimeWithoutProcessing": 1,
      "Cron": "*/1 * * * *"
    },
    "Question": {
      "MessagesPerCycle": 50,
      "MaxTimeWithoutProcessing": 1,
      "Cron": "*/1 * * * *"
    }
  },
  "Workers": {
    "ElasticUpdateWorker": {
      "Cron": "*/1 * * * *"
    }
  }
}
